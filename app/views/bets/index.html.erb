	<div class="row">

		<p class="text-center"><strong><%= @useSeason %></strong></p>

		<div class="span6 well lead">
			<table class="table table-condensed table-hover">
				<thead>
					<tr><th>Navn</th><th>&nbsp;</th><th>Poeng plassering</th><th>Poeng runde</th><th>Sum</th></thr>
				</thead>
				<% @users_sorted.each do |user|
					if user.hasBets
				%>
					<tbody>
						<tr>
							<td class="lead"><%= user.name %></td>
							<td>&nbsp;</td>
							<td class="lead"><%= user.points %></td>
							<td class="lead"><%= user.round_points %></td>
							<td class="lead"><%= user.total_points %></td>
						</tr>
					</tbody>
				<% end
				 end%>
			</table>
		</div>
	</div>

	<% @users_sorted.each do |user|
		if user.hasBets
	%>

	<div class="row">
		<div class="span3">
		</div>
		<div class="span6 well lead">
			<table class="table table-condensed table-hover">
				<thead>
					<tr>
						<th align="left" colspan=3><%= user.name %></th>
					</tr>
				</thead>
				<tbody>
				<% user.bets.each do |bet|
					if (bet.seasonstartyear == @useSeason)
				%>
					<tr>
						<td class="lead"><%= bet.position %>.</td>
						<td class="lead"><%= bet.league.name %></td>
						<td class="lead"><%= bet.points %></td>
					</tr>
				<% end
				   end %>
					<tr>
						<th class="lead">Sum</th>
						<th class="lead">&nbsp;</th>
						<th class="lead"><%= user.points %></th>
					</tr>
				</tbody>
			</table>
		</div>
	</div>

		<%
		   end
		   end
		%>





